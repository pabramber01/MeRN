{
  "version": 2,
  "buildCommand": "npm run start && npm run populate",
  "installCommand": "npm ci",
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/common/static/(.*)",
      "dest": "public/common/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ]
}
