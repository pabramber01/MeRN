{
  "version": 2,
  "buildCommand": "npm run start && npm run populate",
  "installCommand": "npm ci",
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ]
}
